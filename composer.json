{
    "name": "laravel-notification-channels/clockworksms",
    "description": "A Laravel 5.3 Notification provider for Clockwork SMS.",
    "homepage": "https://github.com/laravel-notification-channels/clockworksms",
    "license": "MIT",
    "authors": [
        {
            "name": "Kitbs",
            "email": "mail@kitbs.com",
            "homepage": "http://kitbs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/notifications": "^5.3",
        "illuminate/support": "^5.1|^5.2|^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\ClockworkSMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\ClockworkSMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
